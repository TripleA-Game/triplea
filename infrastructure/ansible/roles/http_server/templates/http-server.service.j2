[Unit]
Description=TripleA Http Server
Documentation=

[Service]
WorkingDirectory={{ http_server_folder }}
User={{ http_server_user }}
Group={{ http_server_user }}
ExecStart={{ http_server_folder }}/run_server
Restart=always
Environment="ERROR_REPORTING_GITHUB_ORG=triplea-game"
Environment="ERROR_REPORTING_GITHUB_REPO=test"
Environment="GITHUB_API_AUTH_TOKEN=token"

[Install]
WantedBy=multi-user.target
